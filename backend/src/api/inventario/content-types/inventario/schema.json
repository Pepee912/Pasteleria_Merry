{
  "kind": "collectionType",
  "collectionName": "inventarios",
  "info": {
    "singularName": "inventario",
    "pluralName": "inventarios",
    "displayName": "Inventario"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "cantidad_actual": {
      "type": "integer",
      "required": true
    },
    "unidad_medida": {
      "type": "string",
      "required": true
    },
    "ultima_actualizacion": {
      "type": "datetime"
    },
    "producto": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::producto.producto",
      "inversedBy": "inventario"
    },
    "estado": {
      "type": "enumeration",
      "enum": [
        "disponible",
        "agotado",
        "inactivo"
      ]
    }
  }
}
