{
  "kind": "collectionType",
  "collectionName": "pedidos",
  "info": {
    "singularName": "pedido",
    "pluralName": "pedidos",
    "displayName": "Pedido"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "estado": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "pendiente, ",
        "aceptado, ",
        "entregado,",
        "cancelado"
      ]
    },
    "fecha_pedido": {
      "type": "datetime",
      "required": true
    },
    "fecha_entrega": {
      "type": "datetime"
    },
    "total": {
      "type": "decimal",
      "required": true
    },
    "notas": {
      "type": "richtext"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "pedidos"
    },
    "detalles_pedidos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::detalles-pedido.detalles-pedido",
      "mappedBy": "pedido"
    },
    "venta": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::venta.venta",
      "mappedBy": "pedido"
    }
  }
}
